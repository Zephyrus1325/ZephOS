{
    "configurations": [
        {
            "name": "windows-gcc-x64",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerPath": "gcc",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerArgs": [
                ""
            ]
        },
        {
            "name": "Arm-GCC",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-mcpu=cortex-a9 ",
                "-mfloat-abi=softfp ",
                "-mfpu=neon-fp16 \\",
                "-ffreestanding ",
                "-nostdlib -g -O0 -I. "
            ]
        }
    ],
    "version": 4
}